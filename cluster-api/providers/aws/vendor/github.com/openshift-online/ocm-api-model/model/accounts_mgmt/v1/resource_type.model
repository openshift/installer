// Identifies computing resources
class Resource {
    SKU String
	// platform-specific name, such as "M5.2Xlarge" for a type of EC2 node
	ResourceName String
	ResourceType String
	BYOC Boolean
	AvailabilityZoneType String
	// Number of allowed nodes
	Allowed Integer
}
