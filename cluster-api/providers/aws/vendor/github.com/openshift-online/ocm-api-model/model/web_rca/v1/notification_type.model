// Definition of a Web RCA notification.
class Notification {
  // Object creation timestamp.
  CreatedAt Date

  // Object modification timestamp.
  UpdatedAt Date

  // Object deletion timestamp.
  DeletedAt Date

  Name String
  Incident Incident
  Checked Boolean
  Rank Integer
}
